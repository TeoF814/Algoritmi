#include <iostream>
using namespace std;

int main()
{
	int p[100];
	for (int i = 0;i < 100;i++) {
		p[i] = rand() % 100;
	}
	int n = 0;
	while (n < 100) {
		for (int i = 0;i < 99;i++) {
			if (p[i] < p[i + 1]) {
				int pomoc = p[i];
				p[i] = p[i + 1];
				p[i + 1] = pomoc;
			}
		}
		n++;
	}
	int x;
	cin >> x;
	int brojac = 0;
	for (int i = 0;i < x;i++) {
		brojac += p[i];
	}
	cout << brojac;
}
