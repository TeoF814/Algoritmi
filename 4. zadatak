#include <iostream>
using namespace std;

int main()
{
	char p[100];
	for (int i = 0;i < 100;i++) {
		p[i] = 'a' + rand() % 26;
	}
	int n = 0;
	while (n < 100) {
		for (int i = 0;i < 99;i++) {
			if (p[i] > p[i + 1]) {
				int pomoc = p[i];
				p[i] = p[i + 1];
				p[i + 1] = pomoc;
			}
		}
		n++;
	}

	cout << p[97] << p[98] << p[99];
}
