#include <iostream>
using namespace std;

int main()
{
	int p[300];
	for (int i = 0;i < 300;i++) {
		p[i] = rand() % 300;
	}

	int n = 0;
	while (n < 300) {
		for (int i = 0;i < 299;i++) {
			if (p[i] > p[i + 1]) {
				int pomoc = p[i];
				p[i] = p[i + 1];
				p[i + 1] = pomoc;
			}
		}
		n++;
	}
	int x;
	cin >> x;
	int d = 100;
	int g = 199;
	bool trazen = false;
	while (1) {
		int s = (d + g) / 2;
		if (d > g) {
			break;
		}
		if (p[s] == x) {
			trazen = true;
			break;
		}
		if (p[s] > x) {
			g = s - 1;
		}
		if (p[s] < x) {
			d = s + 1;
		}
	}
	cout << trazen;
}
